{
	"name": "@nineone/boilerplate-client",
	"version": "1.0.0",
	"license": "Private",
	"private": true,
	"scripts": {
		"pre-dev": "yarn run clean",
		"dev": "nodemon",
		"pre-build": "yarn run clean",
		"build": "cross-env NODE_ENV=production next build && cross-env NODE_ENV=production yarn ts-node --project tsconfig.server.json server/index.ts",
		"start": "cross-env NODE_ENV=production node dist/index.js",
		"clean": "rimraf .next",
		"tslint": "tslint -c tslint.json -p tsconfig.json"
	},
	"cacheDirectories": [
		"node_modules"
	],
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"**/*.{ts,tsx}": [
			"yarn tslint -c tslint.json -t prose -p tsconfig.json --fix",
			"git add"
		]
	},
	"dependencies": {
		"@emotion/core": "^10.0.10",
		"@emotion/styled": "^10.0.12",
		"@zeit/next-css": "^1.0.1",
		"@zeit/next-typescript": "^1.1.1",
		"babel-plugin-emotion": "^10.0.13",
		"emotion": "^10.0.9",
		"emotion-theming": "^10.0.10",
		"express": "^4.17.1",
		"isomorphic-unfetch": "^3.0.0",
		"mobx": "^5.10.1",
		"mobx-react": "^6.0.3",
		"mobx-state-tree": "^3.14.0",
		"next": "^8.1.0",
		"next-compose-plugins": "^2.2.0",
		"next-offline": "^4.0.2",
		"next-optimized-images": "^2.5.1",
		"next-progressbar": "^1.0.0",
		"next-seo": "^1.11.2",
		"polished": "^3.4.0",
		"prop-types": "^15.7.2",
		"react": "^16.8.6",
		"react-dom": "^16.8.6",
		"recompose": "^0.30.0"
	},
	"devDependencies": {
		"@babel/core": "^7.4.5",
		"@babel/plugin-proposal-decorators": "^7.4.4",
		"@types/express": "^4.17.0",
		"@types/next": "^8.0.5",
		"@types/next-seo": "^1.10.0",
		"@types/node": "11.12.0",
		"@types/react": "^16.8.17",
		"@types/react-dom": "^16.8.4",
		"babel-plugin-polished": "^1.1.0",
		"cross-env": "^5.2.0",
		"fork-ts-checker-webpack-plugin": "^1.3.6",
		"husky": "^2.4.1",
		"imagemin-gifsicle": "^6.0.1",
		"imagemin-mozjpeg": "^8.0.0",
		"imagemin-optipng": "^7.0.0",
		"imagemin-pngquant": "^8.0.0",
		"imagemin-svgo": "^7.0.0",
		"lint-staged": "^8.1.7",
		"nodemon": "^1.19.1",
		"prettier": "^1.18.2",
		"rimraf": "^2.6.3",
		"svg-sprite-loader": "^4.1.6",
		"ts-node": "^8.2.0",
		"tsconfig-paths-webpack-plugin": "^3.2.0",
		"tslint": "^5.16.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.0.1",
		"tslint-react": "^4.0.0",
		"typescript": "^3.4.5",
		"webp-loader": "^0.5.0",
		"webpack": "^4.34.0"
	}
}
