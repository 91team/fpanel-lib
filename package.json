{
  "name": "-boilerplate-",
  "version": "1.0.0",
  "license": "Private",
  "private": true,
  "scripts": {
    "build:development": "yarn clean:development && yarn ts-node -r dotenv/config --project build/tsconfig.json build/index.ts dotenv_config_path=.env.development",
    "build:development:hot": "yarn clean:development && yarn ts-node -r dotenv/config --project build/tsconfig.json build/index.ts dotenv_config_path=.env.development:hot",
    "build:production": "yarn clean:production && yarn ts-node -r dotenv/config --project build/tsconfig.json build/index.ts dotenv_config_path=.env.production",
    "clean:development": "yarn rimraf ./public/webpack/development/*",
    "clean:production": "yarn rimraf ./public/webpack/production/*",
    "gql:generate-configs": "yarn ts-node --project ./scripts/tsconfig.json ./scripts/generateGQLConfigs --config ./src/lib/gqlConfig/config.ts",
    "lint": "yarn tsc --noEmit && yarn eslint 'src' 'build' 'scripts' 'types' --ext ts,tsx",
    "schema:clean": "yarn rimraf schema.graphql && yarn rimraf src/lib/gql/generated",
    "schema:generate-types": "yarn schema:clean && graphql-codegen --config codegen.yml",
    "server:start:development": "yarn ts-node -r dotenv/config --project ./src/server/tsconfig.json ./src/server/index.ts dotenv_config_path=.env.development",
    "server:start:development:hot": "yarn ts-node -r dotenv/config --project ./src/server/tsconfig.json ./src/server/index.ts dotenv_config_path=.env.development:hot",
    "server:start:production": "yarn ts-node -r dotenv/config --project ./src/server/tsconfig.json ./src/server/index.ts dotenv_config_path=.env.production",
    "start": "yarn clean:development && yarn server:start:development",
    "start:hot": "yarn clean:development && yarn server:start:development:hot",
    "start:prod": "yarn clean:production && yarn server:start:production"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">= 12.13.0",
    "yarn": ">= 1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
