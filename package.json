{
  "name": "@nineone/boilerplate-client",
  "version": "1.0.0",
  "license": "Private",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "yarn run clean && next build",
    "start": "next start",
    "build:production": "NODE_ENV=production yarn run build",
    "start:production": "yarn run build:production && NODE_ENV=production next start",
    "clean": "rimraf .next",
    "lint": "yarn tsc --noEmit && yarn eslint src --ext ts,tsx"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">= 12.13.0",
    "yarn": ">= 1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@emotion/core": "^10.0.14",
    "@emotion/styled": "^10.0.14",
    "babel-plugin-emotion": "^10.0.14",
    "emotion": "^10.0.14",
    "emotion-theming": "^10.0.14",
    "isomorphic-unfetch": "^3.0.0",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-utils": "^5.4.1",
    "modern-normalize": "^0.5.0",
    "next": "^9.0.0",
    "next-seo": "^1.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@types/next-seo": "^1.10.0",
    "@types/node": "11.12.0",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-lodash": "^3.3.4",
    "core-js": "^3.6.4",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^9.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.5.4",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "svg-sprite-loader": "4.1.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.5",
    "webp-loader": "^0.5.0",
    "webpack": "^4.35.3"
