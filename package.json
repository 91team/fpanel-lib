{
  "name": "@nineone/boilerplate-client",
  "version": "1.0.0",
  "license": "Private",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "yarn run clean && next build",
    "start": "next start",
    "build:production": "NODE_ENV=production yarn run build",
    "schema:clean": "yarn rimraf schema.graphql && yarn rimraf src/lib/gql/generated",
    "schema:generate-types": "yarn run schema:clean && graphql-codegen --config codegen.yml",
    "start:production": "yarn run build:production && NODE_ENV=production next start",
    "clean": "rimraf .next",
    "lint": "yarn tsc --noEmit && yarn eslint src --ext ts,tsx"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "engines": {
    "node": ">= 12.13.0",
    "yarn": ">= 1.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "3.0.0",
    "@apollo/react-ssr": "3.0.0",
    "apollo-absinthe-upload-link": "^1.5.0",
    "apollo-cache": "^1.3.4",
    "apollo-cache-inmemory": "^1.6.4",
    "apollo-client": "2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-link-logger": "^1.2.3",
    "apollo-utilities": "^1.3.3",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "mobx": "^5.10.1",
    "mobx-react": "^6.1.1",
    "mobx-utils": "^5.4.1",
    "mobx-logger": "^0.7.1",
    "modern-normalize": "^0.5.0",
    "next": "^9.2.1",
    "next-seo": "^1.12.0",
    "polished": "^3.4.2",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^3.1.3",
    "react-dom": "^16.8.6",
    "react-jss": "8.6.1",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/core": "1.8.3",
    "@graphql-codegen/fragment-matcher": "1.8.3",
    "@graphql-codegen/introspection": "1.8.3",
    "@graphql-codegen/schema-ast": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-operations": "1.8.3",
    "@graphql-codegen/typescript-react-apollo": "1.8.3",
    "@types/next-seo": "^1.10.0",
    "@types/node": "11.12.0",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^2.16.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.16.0",
    "@typescript-eslint/parser": "^2.16.0",
    "@zeit/next-css": "^1.0.1",
    "babel-plugin-graphql-tag": "^2.5.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-polished": "^1.1.0",
    "core-js": "^3.6.4",
    "cross-env": "^5.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "file-loader": "^5.0.2",
    "husky": "^3.0.0",
    "imagemin-gifsicle": "^6.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-optipng": "^7.0.0",
    "imagemin-pngquant": "^8.0.0",
    "imagemin-svgo": "^7.0.0",
    "lint-staged": "^9.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "next-compose-plugins": "^2.2.0",
    "next-optimized-images": "^2.5.4",
    "prettier": "^1.18.2",
    "react-jss-hmr": "^1.0.0",
    "rimraf": "^3.0.0",
    "svg-sprite-loader": "4.1.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "tslib": "^1.10.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.1.0",
    "tslint-react": "^4.1.0",
    "tslint-react-hooks": "^2.2.1",
    "typescript": "^3.7.5",
    "url-loader": "^3.0.0",
    "webp-loader": "^0.5.0",
    "webpack": "^4.35.3"
  }
}
